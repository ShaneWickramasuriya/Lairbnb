  <%= simple_form_for [ @lair, @booking ] do |f| %>
    <%= f.input :start_date, input_html: { class: 'dropdown' } %>
    <%= f.input :end_date, input_html: { class: 'dropdown' } %>
    <h5 class="text-center" id="counter" class="" data-count="0"><strong>You haven't picked your dates yet!</strong></h5>
    <div class="text-center"><%= f.submit 'Choose Your Dates!', class: 'btn button blue-button', id: 'paybutton' %></div>
  <% end %>
